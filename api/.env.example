NODE_ENV=development
PORT=4000

# Neon Postgres connection string (recommended)
# Example: postgresql://USER:PASSWORD@HOST/DB?sslmode=require
DATABASE_URL=

# Neon typically requires SSL
DB_SSL=true

# Print sequelize SQL queries (very noisy); keep false normally
DB_LOGGING=false

# Comma-separated allowed origins (ex: http://localhost:3000,https://yourapp.com) or "*" for all
CORS_ORIGINS=*

# Reservation TTL in seconds
RESERVATION_TTL_SECONDS=60

# Expiry worker poll interval (ms)
EXPIRY_POLL_MS=2000

# Logging level (debug|info|warn|error)
LOG_LEVEL=debug
